# shared defaults since both are gmail accounts
defaults
host smtp.gmail.com
port 587
protocol smtp
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account gmail
from kenyonj@gmail.com
user kenyonj@gmail.com
passwordeval "/home/kenyonj/.local/bin/getnetrc kenyonj@gmail.com"

account thoughtbot
from justin@thoughtbot.com
user justin@thoughtbot.com
passwordeval "/home/kenyonj/.local/bin/getnetrc justin@thoughtbot.com"

account default : gmail
